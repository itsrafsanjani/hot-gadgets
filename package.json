{
  "name": "hot-gadget",
  "version": "1.0.0",
  "description": "Hot Gadget is a simple ecommerce website developed with HTML, CSS and Bootstrap. Feel free to modify.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",

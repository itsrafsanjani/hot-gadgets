  "bugs": {
    "url": "https://github.com/itsrafsanjani/hot-gadget/issues"
  },
  "homepage": "https://github.com/itsrafsanjani/hot-gadget#readme",
  "dependencies": {
    "bootstrap": "^4.6.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "tinyify": "^3.0.0",
    "watchify": "^4.0.0"
  }
